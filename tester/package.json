{
  "name": "tester",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "postinstall": "react-native unpack-harmony",
    "pack:pkg": "cd ../react-native-harmony-reanimated && npm pack && cd ../tester",
    "install:pkg": "npm uninstall react-native-harmony-reanimated && npm run pack:pkg && npm i react-native-harmony-reanimated@file:../react-native-harmony-reanimated/rnoh-react-native-harmony-reanimated-3.6.0.tgz",
    "dev": "npm run install:pkg && react-native bundle-harmony",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.5",
    "react-native-harmony": "npm:@rnoh/react-native-harmony@^0.72.13",
    "react-native-harmony-reanimated": "file:../react-native-harmony-reanimated/rnoh-react-native-harmony-reanimated-3.6.0.tgz",
    "react-native-reanimated": "3.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
